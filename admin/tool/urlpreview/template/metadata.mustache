{{#noogmetadata}}
  <p>No Open Graph metadata available.</p>
{{/noogmetadata}}

{{^noogmetadata}}
  <div class="unfurl-metadata">
    {{#title}}
      <h2>Title: {{.}}</h2>
    {{/title}}
    
    {{#sitename}}
      <p>Site Name: {{.}}</p>
    {{/sitename}}
    
    {{#image}}
      <img src="{{.}}" alt="Site image">
    {{/image}}
    
    {{#description}}
      <p>Description: {{.}}</p>
    {{/description}}
    
    {{#canonicalurl}}
      <p>Canonical URL: <a href="{{.}}">{{.}}</a></p>
    {{/canonicalurl}}
  </div>
{{/noogmetadata}}
